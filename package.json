{
  "name": "@cuiqg/eslint-config",
  "version": "2.8.17",
  "description": "Eslint config for @cuiqg",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://eslint-config.tsuiqg.me",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuiqg/eslint-config.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Unlicense",
  "author": "Tsuiqg <i@tsuiqg.me>",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "build:inspect": "npx @eslint/config-inspector build --config eslint-inspector.config.js",
    "lint": "eslint .",
    "lint:inspect": "npx @eslint/config-inspector --open false --config eslint-inspector.config.js",
    "prepublishOnly": "npm run build",
    "release": "bumpp"
  },
  "peerDependencies": {
    "eslint": ">=9.28.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "tsdown": "^0.18.2",
    "typescript": "^5.9.3",
    "unrun": "^0.2.22"
  },
  "dependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.1",
    "@poupe/eslint-plugin-tailwindcss": "^0.2.4",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@unocss/eslint-plugin": "^66.5.10",
    "@vue-macros/eslint-config": "3.0.0-beta.21",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-plugin-baseline-js": "^0.4.2",
    "eslint-plugin-depend": "^1.4.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-package-json": "^0.85.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "^2.4.2",
    "local-pkg": "^1.1.2",
    "vue-eslint-parser": "^10.2.0"
  },
  "packageManager": "pnpm@10.27.0"
}
