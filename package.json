{
  "name": "@cuiqg/eslint-config",
  "version": "2.5.8",
  "description": "Eslint config for @cuiqg",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://eslint.tsuiqg.me",
  "repository": "github:cuiqg/eslint-config",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Unlicense",
  "author": "Tsuiqg <i@tsuiqg.me>",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "build:inspect": "eslint-config-inspector build --config eslint-inspector.config.js",
    "lint": "eslint .",
    "lint:inspect": "eslint-config-inspector --open false --config eslint-inspector.config.js",
    "prepack": "pnpm build",
    "release": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@cuiqg/prettier-config": "latest",
    "@eslint/config-inspector": "^1.4.2",
    "@eslint/eslintrc": "^3.3.3",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "tsdown": "^0.16.8"
  },
  "peerDependencies": {
    "eslint": ">=9.28.0"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@unocss/eslint-plugin": "^66.5.9",
    "@vue-macros/eslint-config": "3.0.0-beta.21",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-package-json": "^0.85.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "^2.4.1",
    "local-pkg": "^1.1.2",
    "vue-eslint-parser": "^10.2.0"
  },
  "packageManager": "pnpm@10.24.0"

}
