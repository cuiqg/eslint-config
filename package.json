{
  "name": "@cuiqg/eslint-config",
  "version": "2.4.0",
  "description": "Eslint config for @cuiqg",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://eslint.tsuiqg.me",
  "repository": "github:cuiqg/eslint-config",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Unlicense",
  "author": "Tsuiqg <i@tsuiqg.me>",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "build:inspect": "pnpm build && eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:inspect": "eslint-config-inspector --open false --config eslint-inspector.config.ts",
    "prepack": "pnpm build",
    "release": "bumpp && pnpm publish",
    "test": "vitest --ui",
    "typecheck": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@cuiqg/prettier-config": "latest",
    "@cuiqg/typescript-config": "^0.1.2",
    "@eslint/config-inspector": "^1.1.0",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.2.0",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "tsdown": "^0.13.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=9.28.0"
  },
  "dependencies": {
    "@eslint/js": "^9.32.0",
    "@next/eslint-plugin-next": "^15.4.6",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@unocss/eslint-plugin": "^66.4.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-flat-config-utils": "^2.1.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-de-morgan": "^1.3.1",
    "eslint-plugin-depend": "^1.2.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^52.0.4",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-package-json": "^0.52.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.3.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "vue-eslint-parser": "^10.2.0"
  },
  "packageManager": "pnpm@10.14.0",
  "prettier": "@cuiqg/prettier-config",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ]
}
